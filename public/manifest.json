{
  "manifest_version": 3,
  "short_name": "ETH extension",
  "name": "My ETH Extension",
  "version": "1.0",
  "description": "A Chrome extension built with React, TypeScript, and Material UI.",
  "action": {
    "default_popup": "index.html",
    "default_icon": "icon.png"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "permissions": ["storage"],
  "host_permissions": ["<all_urls>"]
}
